<?xml version="1.0"?>
<package format="3">
  <name>uav_perception</name>
  <version>0.0.1</version>
  <description>Modular UAV perception nodes</description>
  <maintainer email="your_email@example.com">level6</maintainer>
  <license>MIT</license>

  <buildtool_depend>ament_python</buildtool_depend>

  <exec_depend>rclpy</exec_depend>
